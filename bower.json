{
  "name": "music-app",
  "dependencies": {
    "bootstrap": "~3",
    "bootstrap-sweetalert": "~0",
    "font-awesome": "~4",
    "jquery": "~2",
    "lesshat": "~3",
    "jquery-pjax":"~1",
    "moment": "~2",
    "slimScroll": "1.3.3",
    "nprogress": "https://github.com/lipis/nprogress.git#master",
    "chosen":"https://github.com/harvesthq/chosen/releases/download/v1.4.1/chosen_v1.4.1.zip",
    "jplayer":"2.9.2",
    "simple-line-icons":"1.0.0"
  },
  "overrides": {
    "bootstrap": {
      "main": ["less/**", "fonts/*", "js/*"]
    },
    "bootstrap-sweetalert": {
      "main": ["lib/*.less", "lib/sweet-alert.js"]
    },
    "font-awesome": {
      "main": ["less/*", "fonts/*"]
    },
    "jplayer":{
      "main":[
        "dist/jplayer/jquery.jplayer.js",
        "dist/jplayer/jquery.jplayer.swf",
        "dist/add-on/jplayer.playlist.js"
      ]
    }
  }
}
